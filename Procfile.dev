redis-server: redis-server

web: env LOG_CONSOLE=1 WEB_CONCURRENCY=1 WEB=1 bundle exec unicorn -p $PORT -c ./config/unicorn.rb
assets: env WEB=1 WEB_CONCURRENCY=5 bundle exec unicorn -p 5111 -c ./config/unicorn.rb
worker: env LOG_CONSOLE=1 bundle exec sidekiq -c 5 -q indexing,3 -q download,1 -q upload,1 -q notification,1 -q jbb_synch,1 -q export,1

guard-livereload: bundle exec guard
search-solr: bundle exec rake sunspot:solr:run
