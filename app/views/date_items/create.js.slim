- if resource.errors.any?
  = render 'form_dialog'
- else
  |
    $('.bootbox.modal').modal('hide');
    if (typeof dateitemsTable != 'undefined'){
      dateitemsTable.fnDestroy();
    }
    $('table#date_range-dates').replaceWith('#{escape_javascript(render partial: 'date_ranges/date_items', locals: { resource: parent, date_items: parent.date_items })}');