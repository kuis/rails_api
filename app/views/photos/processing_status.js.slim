- @photos.each do |photo|
  - if photo.processed?
    |
      $('#processed_asset_#{photo.id}').remove()
      $('#attached_asset_#{photo.id}').replaceWith('#{j render(partial: 'results/photos/photo', locals: {photo: photo})}').show();
  - else
    |
      $('#processed_asset_#{photo.id}').find('.progress .bar').css({ width: #{photo.upload_percentage} + '%' });