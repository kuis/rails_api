        br>/
        
        span= number_with_precision(dashboard_kpis_trends_data[:impressions_event], precision: 2)
        | IMPRESSIONS/EVENT
        span.sparkline data-sparkline="bar" data-values="0,#{30.times.map{ 3 + Random.rand(6) }.join(',')}"
        br>/
        
        span= number_with_precision(dashboard_kpis_trends_data[:interactions_event], precision: 2)
