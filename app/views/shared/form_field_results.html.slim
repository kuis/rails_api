.form-results-box
  = render partial: 'shared/form_field_results', locals: { form_fields: form_fields }, formats: [:html]

- content_for :pdf_scripts
  = wicked_pdf_javascript_include_tag 'jquery'
  = wicked_pdf_javascript_include_tag 'jquery-ui-1.10.4.custom'
  = wicked_pdf_javascript_include_tag 'bootstrap'
  = wicked_pdf_javascript_include_tag 'jquery.photoGallery'
  = wicked_pdf_javascript_include_tag 'highstock.src.js'
  javascript:
    Highcharts.setOptions({
        plotOptions: {
            series: {
                animation: false
            }
        }
    });
