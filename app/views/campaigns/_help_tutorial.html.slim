  //Staff
  div data-target="#add-staff-btn" data-intro="Add users and teams to the campaign" data-position="right" data-tooltip-style="top: -2px; right: -110px;"
  div data-target="#staff_tab .goalable-list li:nth-child(1) .kpi-goal-field" data-intro="Set goals against campaign KPIs for individual staff" data-position="left" data-tooltip-style="left: -144px; bottom: -3px; height: 48px; max-width: 225px;"
  div data-target="#staff_tab .goalable-list li:nth-child(1) .icon-remove-sign" data-intro="Remove staff from a campaign" data-position="left" data-tooltip-style="left: -108px; bottom: -10px; height: 48px; width: 190px;"
  //Date Ranges
  div data-target="#add-date-range-btn" data-intro="Add approved ranges of dates to the campaign" data-position="right" data-tooltip-style="top: -2px; right: -110px;"
  div data-target="#date_ranges_tab .goalable-list li:nth-child(1) .kpi-goal-field" data-intro="Set goals against campaign KPIs for individual date ranges" data-position="left" data-tooltip-style="left: -144px; bottom: -3px; height: 48px; max-width: 225px;"
  div data-target="#date_ranges_tab .goalable-list li:nth-child(1) .icon-remove-sign" data-intro="Remove date ranges from a campaign" data-position="left" data-tooltip-style="left: -108px; bottom: -10px; height: 48px; width: 190px;"
  //Day Parts
  div data-target="#add-day-part-btn" data-intro="Add ranges of hours within a day to the campaign" data-position="right" data-tooltip-style="top: -16px; right: -110px;"
  div data-target="#day_parts_tab .goalable-list li:nth-child(1) .kpi-goal-field" data-intro="Set goals against campaign KPIs for individual day parts" data-position="left" data-tooltip-style="left: -144px; bottom: -3px; height: 48px; max-width: 225px;"
  div data-target="#day_parts_tab .goalable-list li:nth-child(1) .icon-remove-sign" data-intro="Remove day parts from a campaign" data-position="left" data-tooltip-style="left: -108px; bottom: -10px; height: 48px; width: 190px;"
  //Documents
  div data-target="#campaign-documents" data-intro="View any documents that have been added to the campaign. These documents appear for event event scheduled for the campaign." data-position="top" data-helper-class="chardinjs-dashed" data-element-class="chardinjs-keep-back" data-tooltip-style="top: -95px; right: 70px; max-width: 269px;"
  div data-target="#campaign-documents-list .remove" data-intro="Remove documents from a campaign" data-position="top" data-tooltip-style="left: -142px; bottom: 88px; max-width: 290px; width: 283px;"
  div data-target="#campaign-documents .icon-plus-sign" data-intro="Add reference and supplemental documents to a campaign" data-position="bottom" data-tooltip-style="left: -66px; bottom: -63px; width: 200px; padding: 0;"