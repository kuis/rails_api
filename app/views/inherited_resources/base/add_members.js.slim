- if @member_id
  |
    $(document).trigger('members:change');
    if ($('ul#staff-list > li#staff-member-user-#{@member_id}').length){
      $('ul#staff-list > li#staff-member-user-#{@member_id}').remove();
    };
- if @team_id
  |
    $(document).trigger('teams:change');
    if ($('ul#staff-list > li#staff-member-team-#{@team_id}').length){
      $('ul#staff-list > li#staff-member-team-#{@team_id}').remove();
    };

= render 'add_member'