- if resource.is_a?(Event)
  |
    $('#event-team-members').html('#{escape_javascript(render('team_member_list'))}');
- elsif resource.is_a?(Team)
  |
    teamMembersTable.tableScroller('reloadData');
- elsif resource.is_a?(Campaign)
  |
    staffMembersTable.tableScroller('reloadData');
|

- @members.each do |member|
  |
    $('table#select-users-list tr#user-#{member.id}').remove();